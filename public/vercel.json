{
    "version": 2,
    "error": {
        "404": "/404.html"
    },
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "meta",
              "value": "description=Akuut UI/UX Design - Wouter Waumans"
            }
          ]
        }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/api/$1"
        },
        {
          "src": "/(.*)",
          "dest": "/$1",
          "status": 404,
          "headers": {
            "Cache-Control": "public, max-age=3600, must-revalidate"
          }
        }
    ],
    "cleanUrls": true
}